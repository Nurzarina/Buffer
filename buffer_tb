#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x5616ff27d4b0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x5616ff27d640 .scope module, "buffer_tb" "buffer_tb" 3 3;
 .timescale -12 -12;
v0x5616ff290d70_0 .var "clk", 0 0;
v0x5616ff290e40_0 .var "inp", 0 0;
v0x5616ff290f10_0 .net "out", 0 0, v0x5616ff290c50_0;  1 drivers
S_0x5616ff243cf0 .scope module, "uut" "buffer" 3 9, 4 1 0, S_0x5616ff27d640;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "inp";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "out";
v0x5616ff2457b0_0 .net "clk", 0 0, v0x5616ff290d70_0;  1 drivers
v0x5616ff245ba0_0 .net "inp", 0 0, v0x5616ff290e40_0;  1 drivers
v0x5616ff290c50_0 .var "out", 0 0;
E_0x5616ff244400 .event posedge, v0x5616ff2457b0_0;
    .scope S_0x5616ff243cf0;
T_0 ;
    %wait E_0x5616ff244400;
    %load/vec4 v0x5616ff245ba0_0;
    %assign/vec4 v0x5616ff290c50_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5616ff27d640;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5616ff290d70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5616ff290e40_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5616ff27d640;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5616ff290d70_0;
    %inv;
    %store/vec4 v0x5616ff290d70_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5616ff27d640;
T_3 ;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5616ff290e40_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5616ff290e40_0, 0, 1;
    %delay 40, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5616ff290e40_0, 0, 1;
    %delay 60, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5616ff290e40_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 32 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5616ff27d640;
T_4 ;
    %vpi_call/w 3 36 "$monitor", "Time: %0d, input: %b, output: %b ", $time, v0x5616ff290e40_0, v0x5616ff290f10_0 {0 0 0};
    %vpi_call/w 3 37 "$dumpfile", "buffer_tb.vcd" {0 0 0};
    %vpi_call/w 3 38 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5616ff27d640 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "buffer_tb.v";
    "buffer.v";
